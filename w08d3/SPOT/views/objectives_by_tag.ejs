<!DOCTYPE html>
<html lang="en">
<head>
  <title>Tags</title>
  <%- include('include/_head.ejs') %>
</head>
<body>
  <%- include('include/_header.ejs') %>
  <h2>Tags</h2>
  <table>
  <colgroup>
    <col style="width:20%">
    <col style="width:30%">
    <!-- <col style="width:45%">
    <col style="width:15%"> -->
  </colgroup>
  <tbody>
  <tr>
    <!-- <th>id</th> -->
    <th>Type</th>
    <th>Question</th>
    <th>Answer</th>
    <th></th>
    <!-- <th>question</th>
    <th>answer</th>
    <th>Feedback<br/>confused:fair:excellent</th> -->
  </tr>
  <% for(var i=0; i < objectives.length; i++) { %>
    <tr id="objective_<%= objectives[i].id %>">
      <!-- <td><%= objectives[i].id %></td> -->
      <td><%= objectives[i].type %></td>
      <td><a href="/objectives/<%= objectives[i].id %>"><%= objectives[i].question %></a>
<a href="/objectives/delete/<%= objectives[i].id %>">[x]</a>
<a href="/objectives/edit/<%= objectives[i].id %>">[e]</a>
      </td>
      <td><pre><%= objectives[i].answer %></pre></td>
      <td><%= objectives[i].understandString %></td>
    </tr>
  <% } %>
  </tbody>
</table>

</body>
</html>
