<!DOCTYPE html>
<html lang="en">
<head>
  <title>New Objective</title>
  <%- include('include/_head.ejs') %>
</head>
<body>
  <%- include('include/_header.ejs') %>
  <form action="/objectives/edit" method="POST">
  <label for="type">Type</label>
  <select id="type" name="type">
    <option value="learning">Learning</option>
    <option value="performance">Performance</option>
  </select>
  <input type="hidden" name="id" value="<%= id %>">
  <label for="question">Question</label>
  <textarea id="question" name="question" rows="10" cols="80"><%= question %></textarea>
  <label for="answer">Answer</label>
  <textarea id="answer" name="answer" rows="10" cols="80"><%= answer %></textarea>
  <label for="answer">SVG Diagram Source</label>
  <textarea id="answer" name="svgdiagram" rows="10" cols="80"><%= svg_diagram %></textarea>
  <label for="sort">Sort</label>
  <input id="sort" name="sort" type="text" value="<%= sort %>"></input>
  <label for="day_id">Day</label>
  <input id="day_id" name="day_id" type="text" value="<%= day_id %>"></input>
  <label for="total_time">Total Time</label>
  <input id="total_time" name="total_time" type="text" value="<%= total_time %>"></input>
  <label for="total_time">Parent ID</label>
  <input id="parent_id" name="parent_id" type="text" value="<%= parent_id %>"></input>
  <input type="submit" name="Save"></input>
</form>
</body>
</html>
